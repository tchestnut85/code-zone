<ol class="post-list">
    {{#each blogs}}
    <li>
        <article class="post">
            <div class="title">
                <span>{{title}}</span>
            </div>
            <div class="meta">
                <span>{{content}}</span>
            </div>
            <div>{{User.username}}</div>
            <div>{{created_at}}</div>
        </article>
    </li>
    {{/each}}
</ol>